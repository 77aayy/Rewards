{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "sync:rewards": "node scripts/inject-firebase-config.js && node scripts/sync-rewards.js",
    "dev": "npm run sync:rewards && npx vite@5",
    "build": "node scripts/inject-firebase-config.js && npx vite@5 build",
    "start": "npx serve -s dist",
    "deploy": "npm run pre-deploy-check && npm run sync:rewards && npm run build && firebase deploy",
    "deploy:preview": "npm run pre-deploy-check && npm run sync:rewards && npm run build && firebase hosting:channel:deploy preview",
    "sync-and-push": "npm run sync:rewards && node -e \"console.log('Then: git add public/rewards && git commit -m \\\"chore: sync rewards\\\" && git push')\"",
    "lint": "eslint .",
    "pre-deploy-check": "node scripts/pre-deploy-check.js",
    "preview": "npx vite@5 preview",
    "deploy-github-firebase": "node scripts/deploy-github-firebase.js",
    "pull-firebase-version": "node scripts/pull-firebase-version.js"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-table": "^8.21.3",
    "firebase": "^12.9.0",
    "lucide-react": "^0.575.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "serve": "^14.2.0",
    "tailwindcss": "^4.1.18",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
